@include@ @includel@../build.mk@includel@

GEOIPDIRS=libGeoIP

.PHONY: all clean distclean $(GEOIPDIRS) dirs

all: dirs geoip.so

geoip.so: geoip.o libGeoIP/GeoIP.o libGeoIP/regionName.o libGeoIP/timeZone.o

dirs: $(GEOIPDIRS)

$(GEOIPDIRS):
	cd $@ && $(MAKE) $(MFLAGS) all

clean:
	rm -f */*.o */*.so *.o *.so

distclean:
	rm -f */Makefile Makefile

