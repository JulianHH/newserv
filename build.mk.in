@ifndef@ INCPATH
INCPATH=../
@endif@

CFLAGS=
CC=gcc
LEX=flex

@sinclude@ @includel@$(INCPATH)settings.mk@includel@

@ifndef@ BUILDID
BUILDID @shell@ (hg id || echo "unknown") | sed -e "s/+ /-/;s/ /-/" @shellend@
@endif@

.SUFFIXES: .so .y .l

.o.so:
	@-ldline-@ $(LDFLAGS)

.y.c:	;

.l.c:	;

CFLAGS+=-Wall -g -finline-functions -funroll-loops -std=c99 -I./ -DBUILDID=$(BUILDID)
CFLAGS+=-fPIC -export-dynamic
